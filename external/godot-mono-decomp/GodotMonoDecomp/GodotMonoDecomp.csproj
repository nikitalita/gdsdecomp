    <ItemGroup>
      <PackageReference Include="ICSharpCode.Decompiler" Version="9.1.0.7988" />
      <PackageReference Include="ICSharpCode.ILSpyX" Version="9.1.0.7988" />
    </ItemGroup>

</Project>
